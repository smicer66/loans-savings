                                                                <a class="dropdown-item js-sweetalert2-reject_sys_user" href="#" style="color: #ff0000" data-id="<%= user.user.id %>"><i class="fal fa-times"></i> Reject</a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <% end %>
                                            </td>
                                        </tr>
                                    <% end %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



