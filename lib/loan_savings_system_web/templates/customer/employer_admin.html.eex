                                        <th>Company</th>
                                        <% end %>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%= for user <- @client_users do %>  
                                        <tr>
                                            <td><%= user.userBioData.lastName %>, <%= user.userBioData.firstName %> <%= user.userBioData.otherName %></td>
                                            <td><%= user.userBioData.emailAddress %></td>
                                            <td><%= user.cl.roleType %></td>
                                            <td><%= user.userBioData.meansOfIdentificationType %></td>
                                            <td><%= user.userBioData.meansOfIdentificationNumber %></td>
                                            <td><%= user.user.username %></td>
                                            <%= if is_nil(@company) do %>
                                            <td><%= user.company.companyName %></td>
                                            <% end %>
                                            <td>
                                                <%= if @current_user_role.roleType == "BANKOFFICE_ADMIN" do %>
                                                <div class="input-group">
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class='subheader-icon fal fa-chevron-down'></i> </span>
                                                            <div class="dropdown-menu">
                                                                <a type="button" class="dropdown-item" href="#"  data-id="<%= user.user.id %>" data-first_name="<%=  user.userBioData.firstName %>" data-last_name="<%= user.userBioData.lastName %>" data-email="<%= user.userBioData.emailAddress %>" data-user_role="<%= user.cl.roleType %>" data-sex ="<%= user.userBioData.gender %>" data-age ="<%= user.userBioData.dateOfBirth %>" data-id_no ="<%= user.userBioData.meansOfIdentificationNumber %>" data-phone="<%= user.user.username %>"  data-toggle="modal" data-target="#view_employer_admin"><i class="fal fa-eye"></i> View</a>
                                                                <a type="button" class="dropdown-item" href="#"  data-id="<%= user.user.id %>" data-first_name="<%=  user.userBioData.firstName %>" data-last_name="<%= user.userBioData.lastName %>" data-email="<%= user.userBioData.emailAddress %>" data-user_role="<%= user.cl.roleType %>" data-sex ="<%= user.userBioData.gender %>" data-age ="<%= user.userBioData.dateOfBirth %>" data-id_no ="<%= user.userBioData.meansOfIdentificationNumber %>" data-phone="<%= user.user.username %>"  data-toggle="modal" data-target="#edit_employer_admin"><i class="fal fa-edit"></i> Edit</a>
                                                                <%= if user.cl.status == "ACTIVE" do %>
                                                                    <a class="dropdown-item js-sweetalert2-disable_user" href="#" style="color: #ff0000" data-id="<%= user.user.id %>"><i class="fal fa-times"></i> Disable</a>
                                                                <% else %>
                                                                <a class="dropdown-item js-sweetalert2-enable_user" href="#" style="color: #008000" data-id="<%= user.user.id %>" ><i class="fal fa-check"></i> Enable</a>
                                                                <% end %>
                                                                <a type="button" class="dropdown-item" href="#"  data-id="<%= user.user.id %>" data-first_name="<%=  user.userBioData.firstName %>" data-last_name="<%= user.userBioData.lastName %>" data-email="<%= user.userBioData.emailAddress %>" data-user_role="<%= user.cl.roleType %>" data-sex ="<%= user.userBioData.gender %>" data-age ="<%= user.userBioData.dateOfBirth %>" data-id_no ="<%= user.userBioData.meansOfIdentificationNumber %>" data-phone="<%= user.user.username %>"  data-toggle="modal" data-target="#view_modal"><i class="fal fa-eye"></i> View</a>
