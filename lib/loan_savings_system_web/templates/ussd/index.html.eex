    function backspace()
    {
        var currentVal = $('#val').val();
        if(currentVal.length >0)
        {
            currentVal = currentVal.substring(0, currentVal.length-1);
            $('#val').val(currentVal);
            $('#displayInput').html(currentVal);

            currentVal = $('#val').val();
